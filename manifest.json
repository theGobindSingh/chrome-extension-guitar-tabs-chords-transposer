{
  "manifest_version": 2,
  "name": "Ultimate-guitar chords transposer",
  "permissions": ["storage", "webRequest", "*://*.ultimate-guitar.com/*"],
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://tabs.ultimate-guitar.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  }
}
